      "source": [
        "def egcd(a,b):\n",
        "  if a==0:\n",
        "    return (b,0,1)\n",
        "  else:\n",
        "    gcd, x,y = egcd (b % a, a)\n",
        "    return (gcd,y-(b//a)*x,x)"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "a=int(input(\"Enter a: \"))\n",
        "b=int(input(\"Enter b: \"))\n",
        "gcd, x, y = egcd(a,b)\n",
        "print(\"GCD: \",gcd, \"\\nx: \",x, \"\\ny: \",y)"
